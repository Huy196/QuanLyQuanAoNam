<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.login.InvoiceAdminController">
    <ListView fx:id="orderListView" prefHeight="300.0" prefWidth="600.0"/>
    <HBox alignment="CENTER" spacing="20">
        <Button fx:id="confirmButton" onAction="#handleConfirm" text="Xác Nhận"/>
        <Button fx:id="rejectButton" onAction="#handleReject" text="Từ Chối"/>
    </HBox>
</VBox>
