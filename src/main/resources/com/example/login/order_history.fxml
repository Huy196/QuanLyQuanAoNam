<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.login.OrderHistoryController">
    <Label fx:id="orderHistoryLabel" prefHeight="31.0" prefWidth="91.0" text="Lịch sử đơn hàng" />
    <ListView fx:id="orderHistoryListView" prefHeight="400" prefWidth="600" />

    <HBox alignment="center" spacing="10">
        <Button fx:id="payButton" onAction="#handlePay" text="Thanh toán" />
        <Button fx:id="cancelButton" onAction="#handleCancel" text="Hủy đơn" />
    </HBox>
</VBox>
